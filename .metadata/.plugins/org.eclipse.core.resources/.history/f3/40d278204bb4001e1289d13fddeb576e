package com.ThreadMethod.Assignments;

public class Demo extends Thread {
	static Thread mt;

	@Override
	public void run() {
		Thread.yield();
		for (int i = 0; i < 10; i++) {
			try {
				//mt.join();
				Thread.sleep(2000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
			System.out.println("Child thread executing");
		}
	}

}
